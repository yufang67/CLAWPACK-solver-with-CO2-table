#
#
# pressure at t10
#
#
READ BLOCK "HLLC.C0000" 
BLOCK xy "9:1"
READ BLOCK "HLLC.C0001" 
BLOCK xy "9:1"
READ BLOCK "HLLC.C0002"
BLOCK xy "9:1"
READ BLOCK "HLLC.C0003"
BLOCK xy "9:1"
#
#G0.S0.y = G0.S0.y / 1e6
#
#
s0 line   color 1
s0 line linewidth 1
s0 line linestyle 1
#
s1 symbol 8
s1 symbol skip 20
s1 symbol size 0.8
s1 symbol color 4
s1 line   color 4
s1 line linewidth 1.5
s1 line linestyle 1	
#
s2 line   color 2
s2 line linewidth 1
s2 line linestyle 2
#
s3 line   color 3
s3 line linewidth 1
s3 line linestyle 3
#
#
legend BOX off
legend BOX LINEWIDTH 0.85
legend CHAR SIZE 0.75
legend loctype view
legend x1 0.745
legend y1 0.82
s0  legend "Initial conditions"
s1  legend "HLLC - t\s output \N =  "
s2  legend "HLLC - t\s output \N =  "
s3  legend "HLLC - t\s output \N =  "
#
#set the thickness of the x and y axis
xaxis bar linewidth 2.0
yaxis bar linewidth 2.0
xaxis label "z  [m]"
xaxis label char size 1.2
yaxis label "Pressure, p   [MPa]"
yaxis label char size 1.2
xaxis  tick major 0.2
xaxis  tick minor ticks 1
xaxis  tick default 6
yaxis  tick major 0.02
#
world xmin  0                        
world xmax  1 
world ymin  5e5
world ymax  6.5e6
#
#
PAGE SIZE 640, 512
PRINT TO "HEM_p_test_1.eps"
DEVICE "EPS" OP "level2"
DEVICE "EPS" DPI 1500 
PRINT
